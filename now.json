{
  "version": 2,
  "name": "frontend",
  "alias": ["neutral.af", "www.neutral.af"],
  "build": {
    "env": {
      "VUE_APP_STRIPE_PUBLIC_KEY": "@stripe_public_key",
      "VUE_APP_HONEYCOMB_PUBLIC_KEY": "@honeycomb_public_key"
    }
  },
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {
      "src": "/(?!estimate|about|js|css|img|fonts|favicon\\.ico|robots\\.txt/?).+",
      "status": 301,
      "headers": { "Location": "/" }
    },
    {
      "src": "/(estimate|about)",
      "dest": "/index.html"
    }
  ],
  "regions": ["bru"],
  "public": false
}
