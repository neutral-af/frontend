{
  "version": 2,
  "name": "frontend",
  "alias": ["www.neutral.af"],
  "build": {
    "env": {
      "PROD_STRIPE_PUBLIC_KEY": "@prod_stripe_public_key",
      "DEV_STRIPE_PUBLIC_KEY": "@dev_stripe_public_key",
      "VUE_APP_HONEYCOMB_PUBLIC_KEY": "@honeycomb_public_key"
    }
  },
  "builds": [
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "regions": ["bru"],
  "public": false
}
