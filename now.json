{
  "version": 2,
  "name": "frontend",
  "alias": ["neutral.af", "www.neutral.af"],
  "build": {
    "env": {
      "VUE_APP_BACKEND_URL": "https://api.neutral.af/graphql",
      "VUE_APP_STRIPE_PUBLIC_KEY": "@stripe_public_key",
      "VUE_APP_HONEYCOMB_PUBLIC_KEY": "@honeycomb_public_key"
    }
  },
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "static/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/robots.txt", "dest": "/static/robots.txt" },
    { "src": "/.well-known/apple-developer-merchantid-domain-association", "dest": "/static/apple-developer-merchandid-domain-association" }
  ],
  "regions": ["bru"],
  "public": false
}
